<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">予約検索</h1>
            </div>
            <!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="/projects/<%= req.project.id %>/home">Home</a>
                    </li>
                    <li class="breadcrumb-item active">予約検索</li>
                </ol>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="card card-default">
                    <div class="card-header">
                        <!-- <h3 class="card-title">Quick Example</h3> -->
                        <div class="toolbar">
                            <!--        Here you can write extra buttons/actions for the toolbar              -->
                            <a class="btn btn-default btn-sm downloadCSV" href="#">
                                <i class="fa fa-download"></i>&nbsp;&nbsp;CSV</a>
                            <a class="btn btn-default btn-sm downloadJson" href="#">
                                <i class="fa fa-download"></i>&nbsp;&nbsp;JSON</a>
                        </div>
                    </div>
                    <!-- /.card-header -->

                    <div class="card-body">
                        <form role="form" method="get">
                            <div class="form-row">
                                <div class="form-group col-md-2">
                                    <label for="ids">予約ID</label>
                                    <input type="text" class="form-control form-control-sm" id="ids" name="ids"
                                        placeholder="予約ID"
                                        value="<%= (Array.isArray(searchConditions.ids)) ? searchConditions.ids.join(',') : '' %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="reservationNumbers">予約番号</label>
                                    <input type="text" class="form-control form-control-sm" id="reservationNumbers"
                                        name="reservationNumbers" placeholder="予約番号"
                                        value="<%= (Array.isArray(searchConditions.reservationNumbers)) ? searchConditions.reservationNumbers.join(',') : '' %>">
                                </div>

                                <div class="form-group col-md-2">
                                    <label for="ids">追加チケットテキスト</label>
                                    <input type="text" class="form-control form-control-sm" id="additionalTicketText"
                                        name="additionalTicketText" placeholder="追加チケットテキスト"
                                        value="<%= searchConditions.additionalTicketText %>">
                                </div>

                                <div class="form-group col-md-2">
                                    <label>予約日時</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control form-control-sm" name="bookingTimeRange"
                                            id="bookingTimeRange"
                                            value="<%= (searchConditions.bookingFrom !== undefined) ? `${moment(searchConditions.bookingFrom).format('YYYY-MM-DDTHH:mm:ssZ')} - ${moment(searchConditions.bookingThrough).format('YYYY-MM-DDTHH:mm:ssZ')}` : '' %>">
                                    </div>
                                    <!-- /.input group -->
                                </div>

                                <div class="form-group col-md-2">
                                    <label>更新日時</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control form-control-sm" name="modifiedTimeRange"
                                            id="modifiedTimeRange"
                                            value="<%= (searchConditions.modifiedFrom !== undefined) ? `${moment(searchConditions.modifiedFrom).format('YYYY-MM-DDTHH:mm:ssZ')} - ${moment(searchConditions.modifiedThrough).format('YYYY-MM-DDTHH:mm:ssZ')}` : '' %>">
                                    </div>
                                    <!-- /.input group -->
                                </div>

                                <div class="form-group col-md-2">
                                    <label>予約ステータス</label>
                                    <select multiple class="form-control form-control-sm" name="reservationStatuses[]">
                                        <% Object.values(ReservationStatusType).forEach(function (reservationStatus) { %>
                                        <% 
                                            var selected = false;
                                            selected = Array.isArray(searchConditions.reservationStatuses) && searchConditions.reservationStatuses.indexOf(reservationStatus) >= 0;
                                            %>
                                        <option value="<%= reservationStatus %>" <% if (selected) { %> selected <% } %>>
                                            <%= reservationStatus %>
                                        </option>
                                        <% }) %>
                                    </select>
                                </div>

                                <div class="form-group col-md-2">
                                    <label for="reservationFor[ids]">予約イベントID</label>
                                    <input type="text" class="form-control form-control-sm" id="reservationFor[ids]"
                                        name="reservationFor[ids]" placeholder="予約イベントID"
                                        value="<%= (Array.isArray(searchConditions.reservationFor.ids)) ? searchConditions.reservationFor.ids.join(',') : '' %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="reservationFor[name]">予約イベント名称</label>
                                    <input type="text" class="form-control form-control-sm" id="reservationFor[name]"
                                        name="reservationFor[name]" placeholder="予約イベント名称"
                                        value="<%= searchConditions.reservationFor.name %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="reservationFor[superEvent][ids]">予約イベントシリーズID</label>
                                    <input type="text" class="form-control form-control-sm"
                                        id="reservationFor[superEvent][ids]" name="reservationFor[superEvent][ids]"
                                        placeholder="予約イベントシリーズID"
                                        value="<%= (Array.isArray(searchConditions.reservationFor.superEvent.ids)) ? searchConditions.reservationFor.superEvent.ids.join(',') : '' %>">
                                </div>

                                <div class="form-group col-md-2">
                                    <label>予約イベント開始日時</label>

                                    <div class="input-group">
                                        <div class="input-group-prepend">
                                            <span class="input-group-text">
                                                <i class="fa fa-calendar"></i>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control form-control-sm"
                                            name="reservationFor[startDateRange]" id="reservationFor[startDateRange]"
                                            value="<%= (searchConditions.reservationFor !== undefined && searchConditions.reservationFor.startFrom !== undefined) ? `${moment(searchConditions.reservationFor.startFrom).format('YYYY-MM-DDTHH:mm:ssZ')} - ${moment(searchConditions.reservationFor.startThrough).format('YYYY-MM-DDTHH:mm:ssZ')}` : '' %>">
                                    </div>
                                    <!-- /.input group -->
                                </div>

                                <div class="form-group col-md-2">
                                    <label for="reservedTicket[ticketedSeat][seatNumbers]">座席番号</label>
                                    <input type="text" class="form-control form-control-sm"
                                        id="reservedTicket[ticketedSeat][seatNumbers]"
                                        name="reservedTicket[ticketedSeat][seatNumbers]" placeholder="座席番号"
                                        value="<%= (Array.isArray(searchConditions.reservedTicket.ticketedSeat.seatNumbers)) ? searchConditions.reservedTicket.ticketedSeat.seatNumbers.join(',') : '' %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="reservedTicket[ticketType][ids]">券種ID</label>
                                    <input type="text" class="form-control form-control-sm"
                                        id="reservedTicket[ticketType][ids]" name="reservedTicket[ticketType][ids]"
                                        placeholder="券種ID"
                                        value="<%= (Array.isArray(searchConditions.reservedTicket.ticketType.ids)) ? searchConditions.reservedTicket.ticketType.ids.join(',') : '' %>">
                                </div>

                                <div class="form-group col-md-2">
                                    <label for="underName[id]">チケットホルダーID</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[id]"
                                        name="underName[id]" placeholder="チケットホルダーID"
                                        value="<%= searchConditions.underName.id %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[name]">Name</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[name]"
                                        name="underName[name]" placeholder="もーしょん(正規表現使用可)"
                                        value="<%= searchConditions.underName.name %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[familyName]">Family Name</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[familyName]"
                                        name="underName[familyName]" placeholder="Family Name(正規表現使用可)"
                                        value="<%= searchConditions.underName.familyName %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[givenName]">Given Name</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[givenName]"
                                        name="underName[givenName]" placeholder="Given Name(正規表現使用可)"
                                        value="<%= searchConditions.underName.givenName %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[email]">Email</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[email]"
                                        name="underName[email]" placeholder="cinerino@example.com(正規表現使用可)"
                                        value="<%= searchConditions.underName.email %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[telephone]">Telephone</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[telephone]"
                                        name="underName[telephone]" placeholder="0312345678(正規表現使用可)"
                                        value="<%= searchConditions.underName.telephone %>">
                                </div>
                                <div class="form-group col-md-2">
                                    <label for="underName[identifier]">識別子</label>
                                    <input type="text" class="form-control form-control-sm" id="underName[identifier]"
                                        name="underName[identifier]" placeholder="username:cinerino"
                                        value="<%= (Array.isArray(searchConditions.underName.identifier.$all)) ? `${searchConditions.underName.identifier.$all[0].name}:${searchConditions.underName.identifier.$all[0].value}` : '' %>">
                                </div>
                            </div>

                        </form>

                        <p class="text-center">検索については <a target="_blank" href="<%= consoleUrl %>">Chevre
                                Console</a> をご利用ください。
                        </p>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->

            </div>
            <!--/.col (left) -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>
<!-- /.content -->

