<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>注文取引詳細</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">
                        <a href="/projects/<%= req.project.id %>/dashboard">Home</a>
                    </li>
                    <li class="breadcrumb-item active">注文取引詳細</li>
                </ol>
            </div>
        </div>
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <!-- <div class="callout callout-info">
                        <h5>
                            <i class="fa fa-info"></i> Note:</h5>
                        This page has been enhanced for printing. Click the print button at the bottom of the invoice to test.
                    </div> -->


                <!-- Main content -->
                <div class="invoice p-3 mb-3">
                    <!-- title row -->
                    <div class="row">
                        <div class="col-12">
                            <h4>
                                <i class="fa fa-film"></i>
                                <%= transaction.seller.name.ja %>
                                <small class="float-right">Date:
                                    <%= moment(transaction.startDate).format('D/M/YYYY') %>
                                </small>
                            </h4>
                        </div>
                        <!-- /.col -->
                    </div>
                    <!-- info row -->
                    <div class="row invoice-info">
                        <div class="col-sm-4 invoice-col">
                            販売者
                            <address>
                                <strong>
                                    <a target="_blank"
                                        href="/projects/<%= req.project.id %>/sellers/<%= transaction.seller.id %>">
                                        <%= transaction.seller.name.ja %>
                                    </a>
                                </strong>
                                <br> Type:
                                <%= transaction.seller.typeOf %>
                                <br> Phone:
                                <%= transaction.seller.telephone %>
                                <br> Email:
                                <%= transaction.seller.email %>
                                <br> Url:
                                <%= transaction.seller.url %>
                            </address>
                        </div>
                        <!-- /.col -->

                        <div class="col-sm-4 invoice-col">
                            Customer
                            <address>
                                <strong>
                                    <% if (transaction.agent.memberOf !== undefined) { %>
                                    <a target="_blank"
                                        href="/projects/<%= req.project.id %>/people/<%= transaction.agent.id %>">
                                        <%= transaction.agent.memberOf.membershipNumber %>
                                    </a>
                                    <% } else {%>
                                    <%= transaction.agent.id %>
                                    <% } %>
                                </strong>
                                <br> Type:
                                <%= transaction.agent.typeOf %>
                                <br> Family Name:
                                <%= transaction.agent.familyName %>
                                <br> Given Name:
                                <%= transaction.agent.givenName %>
                                <br> Phone:
                                <%= transaction.agent.telephone %>
                                <br> Email:
                                <%= transaction.agent.email %>
                            </address>
                        </div>
                        <!-- /.col -->
                        <div class="col-sm-4 invoice-col">
                            <!-- <b>Invoice #007612</b>
                                <br>
                                <br> -->
                            <b>注文:</b>
                            <% if (transaction.result !== undefined) { %>
                            <a target="_blank"
                                href="/projects/<%= req.project.id %>/orders/<%= transaction.result.order.orderNumber %>">
                                <%= transaction.result.order.orderNumber %>
                            </a>
                            <% } %>
                            <!-- <br>
                                    <b>Payment Due:</b> 2/22/2014 -->
                        </div>
                        <!-- /.col -->
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.invoice -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>
<!-- /.content -->

<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <!-- ./col -->
            <div class="col-md-12 no-print">
                <div class="card">
                    <div class="card-header p-2">
                        <ul class="nav nav-pills">
                            <li class="nav-item"><a class="nav-link active" href="#timeline"
                                    data-toggle="tab">Timeline</a></li>
                            <li class="nav-item"><a class="nav-link" href="#jsonViewer" data-toggle="tab">JSON
                                    Viewer</a></li>
                            <li class="nav-item"><a class="nav-link" href="#activity" data-toggle="tab">activity</a>
                            </li>
                        </ul>
                    </div><!-- /.card-header -->
                    <div class="card-body">
                        <div class="tab-content">
                            <div class="active tab-pane" id="timeline">
                                <%- include('../../parts/timelines', {timelines: timelines}) %>
                            </div>
                            <!-- /.tab-pane -->

                            <div class="tab-pane" id="jsonViewer">
                                <div class="form-group">
                                    <textarea class="form-control" rows="20" placeholder=""
                                        disabled=""><%= JSON.stringify(transaction, null, '\t') %></textarea>
                                </div>
                            </div>
                            <!-- /.tab-pane -->

                            <div class="tab-pane" id="activity">
                            </div>
                            <!-- /.tab-pane -->
                        </div>
                        <!-- /.tab-content -->
                    </div><!-- /.card-body -->
                </div>
                <!-- /.nav-tabs-custom -->
            </div>
            <!-- ./col -->
        </div>
        <!-- /.row -->
    </div><!-- /.container-fluid -->
</section>
<!-- /.content -->

<!-- DataTables -->
<link rel="stylesheet" href="/node_modules/admin-lte/plugins/datatables-bs4/css/dataTables.bootstrap4.css">

<!-- ChartJS 1.0.1 -->
<script src="/node_modules/admin-lte/plugins/chart.js/Chart.min.js"></script>